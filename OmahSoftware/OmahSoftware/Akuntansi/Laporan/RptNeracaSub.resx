  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT A.kodekategori,A.namakategori,A.kodesubkategori,A.namasubkategori,A.kodetampil,A.namatampil,SUM(A.saldo) AS saldo
FROM (
	SELECT A.kodekategori,A.namakategori,A.kodesubkategori,A.namasubkategori,
		   IFNULL(A.kodesubgrup,IFNULL(A.kodegrup,A.kodesubkategori)) AS kodetampil,
		   IFNULL(A.namasubgrup,IFNULL(A.namagrup,A.namasubkategori)) AS namatampil,
		   A.saldo
	FROM (
		SELECT B.kode AS kodekategori, B.nama AS namakategori, C.kode AS kodesubkategori, C.nama AS namasubkategori, 
			   COALESCE(D.kode,NULL) AS kodegrup, COALESCE(D.nama,NULL) AS namagrup, COALESCE(E.kode,NULL) AS kodesubgrup, COALESCE(E.nama,NULL) AS namasubgrup,
			   CASE WHEN A.saldonormalneraca = 'Debit' THEN COALESCE(F.akhirdebit,0) - COALESCE(F.akhirkredit,0) ELSE COALESCE(F.akhirkredit,0) - COALESCE(F.akhirdebit,0) END AS saldo
		FROM akun A
		INNER JOIN akunkategori B ON A.akunkategori = B.kode
		INNER JOIN akunsubkategori C ON A.akunsubkategori = C.kode
		LEFT JOIN akungroup D ON A.akungroup = D.kode
		LEFT JOIN akunsubgroup E ON A.akunsubgroup = E.kode
		LEFT JOIN saldoakunbulanan F ON A.kode = F.akun AND F.periode = @periode
		WHERE CASE WHEN @jenisakun = 'ASET' THEN B.kode = '1000.00' ELSE  B.kode IN ('2000.00','3000.00') END
	) A
) A
GROUP BY A.kodekategori,A.kodesubkategori,A.kodetampil
ORDER BY A.kodekategori,A.kodesubkategori,A.kodetampil
</value>
